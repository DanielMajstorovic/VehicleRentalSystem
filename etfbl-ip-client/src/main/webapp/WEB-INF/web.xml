<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>etfbl-ip-client</display-name>
  

    <session-config>
        <session-timeout>30</session-timeout> <!-- min -->
    </session-config>

    <welcome-file-list>
        <welcome-file>login</welcome-file>
    </welcome-file-list>
    
    <filter-mapping>
        <filter-name>AuthFilter</filter-name>
        <url-pattern>/home</url-pattern>
        <url-pattern>/car</url-pattern>
        <url-pattern>/ebike</url-pattern>
        <url-pattern>/escooter</url-pattern>
        <url-pattern>/active-rental</url-pattern>
        <url-pattern>/profile</url-pattern>
    </filter-mapping>
</web-app>